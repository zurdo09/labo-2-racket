(display "**** Ciencias de la Computacion - 2018 ****\n")
(display "**** Laboratorio #2 - Ejercicio #1 ****\n\n")
(display "Ingrese Nombre del libro a leer: ")
(define libro(read-line))
(display "Ingrese cantidad de paginas: ")
(define paginas(read))
(display "Ingrese cantidad de dias: ")
(define dias(read))
(define resultado(quotient paginas dias))
(display (string-append "Para leer el libro " libro " en " (number->string dias) " dias debe leer por lo menos " (number->string resultado) " paginas al dia."))